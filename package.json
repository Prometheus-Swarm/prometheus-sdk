{
    "name": "prometheus-swarm-sdk",
    "version": "0.1.0",
    "description": "Official TypeScript/JavaScript SDK for the Prometheus Swarm platform. Create bounties, manage swarm tasks, and interact with the decentralized workforce platform programmatically.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
      "dist/**/*",
      "README.md",
      "LICENSE"
    ],
    "scripts": {
      "build": "tsc",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "lint": "eslint src/**/*.ts",
      "lint:fix": "eslint src/**/*.ts --fix",
      "prepublishOnly": "npm run build && npm test",
      "clean": "rm -rf dist",
      "dev": "tsc --watch"
    },
    "keywords": [
      "prometheus",
      "swarm",
      "bounty",
      "blockchain",
      "decentralized",
      "workforce",
      "api",
      "sdk",
      "typescript",
      "javascript",
      "koii",
      "web3"
    ],
    "author": {
      "name": "Prometheus Team",
      "email": "team@prometheusswarm.ai",
      "url": "https://prometheusswarm.ai"
    },
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/prometheus-swarm/sdk.git"
    },
    "bugs": {
      "url": "https://github.com/prometheus-swarm/sdk/issues"
    },
    "homepage": "https://github.com/prometheus-swarm/sdk#readme",
    "engines": {
      "node": ">=16.0.0"
    },
    "dependencies": {
      "cross-fetch": "^4.0.0"
    },
    "devDependencies": {
      "@types/jest": "^29.5.12",
      "@types/node": "^20.11.24",
      "@typescript-eslint/eslint-plugin": "^7.1.1",
      "@typescript-eslint/parser": "^7.1.1",
      "eslint": "^8.57.0",
      "jest": "^29.7.0",
      "ts-jest": "^29.1.2",
      "typescript": "^5.4.2"
    },
    "publishConfig": {
      "access": "public"
    }
  }